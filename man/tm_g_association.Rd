})

app <- init(
  data = data,
  modules = modules(
    tm_g_association(
      ref = picks(
        datasets("CO2"),
        variables(
          choices = c("Plant", "Type", "Treatment"),
          selected = "Plant"
        )
      ),
      vars = picks(
        datasets("CO2"),
        variables(
